{
  "name": "@jds/npm_vite",
  "version": "1.0.0",
  "type": "module",
  "repository": "https://github.com/jquery-dlya-slabih/npm_vite",
  "homepage": "https://github.com/jquery-dlya-slabih/npm_vite",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
